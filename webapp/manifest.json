{
	"_version": "1.7.0",
	"sap.app": {
		"id": "aweawesome",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"oData.svc": {
				"uri": "/Northwind/v2/oData/oData.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "aweawesome.view.Main",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "aweawesome.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneTime",
					"defaultCountMode": "Request",
					"useBatch": false
				},
				"dataSource": "oData.svc",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "aweawesome.view.Main",
				"controlId": "app01",
				"controlAggregation": "pages",
				"bypassed": {
					"target": "notFoundTarget"
				}
			},
			"routes": [{
				"pattern": "",
				"name": "mainRoute",
				"target": "mainTarget"
			}, {
				"pattern": "detail",
				"name": "detail",
				"target": "DetailProduct"
			}, {
				"pattern": "Products/{ID}",
				"name": "detailProductTarget",
				"target": "detailProductTarget"
			}],
			"targets": {
				"masterTarget": {
					"viewName": "Main",
					"viewLevel": 1
				},
				"detailTarget": {
					"viewName": "Detail",
					"viewLevel": 2,
					"transition": "flip"
				},
				"detailProductTarget": {
					"viewName": "DetailProduct",
					"viewLevel": 2
				}
			}
		}

	}
}